{
  "name": "tagioalisi-web",
  "version": "1.0.0",
  "type": "module",
  "description": "Web UI for the Tagioalisi Bot",
  "scripts": {
    "webpack:esm": "ts-node-esm node_modules/.bin/webpack-cli",
    "webpack-dev-server:esm": "ts-node-esm node_modules/.bin/webpack-dev-server",
    "build": "yarn run build:prod",
    "build:dev": "yarn run webpack:esm --mode development",
    "build:prod": "yarn run webpack:esm --mode production",
    "start": "yarn run start:dev",
    "start:prod": "yarn run webpack-dev-server:esm --mode production",
    "start:dev": "yarn run webpack-dev-server:esm --mode development",
    "analyze:dev": "WEBPACK_ANALYZER=1 yarn run webpack:esm --mode development",
    "analyze:prod": "WEBPACK_ANALYZER=1 yarn run webpack:esm --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fsufitch/tagioalisi-web.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fsufitch/tagioalisi-web/issues"
  },
  "homepage": "https://github.com/fsufitch/tagioalisi-web#readme",
