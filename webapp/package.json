    "@mui/material": "^5.11.10",
    "nice-grpc": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "sass": "^1.58.0",
    "ts-proto": "^1.139.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
